#include <bits/stdc++.h>
using namespace std;
 
 
int main(){
//    freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
    long long int x1,x2,x3,y1,y2,y3,res;
    cin >> x1 >> y1 >> x2 >> y2 >> x3 >> y3;
    res = (y2 - y1) * (x3 - x2) - (y3 - y2) * (x2 - x1);
     if(res == 0){
         cout << "TOUCH" << "\n";
     }else if(res < 0){
         cout << "LEFT" << "\n";
     }else if(res > 0){
         cout << "RIGHT" << "\n";
     }
//    fclose(stdin); fclose(stdout);
    return 0;
}
