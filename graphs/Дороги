#include <bits/stdc++.h>
using namespace std;
 
int main(){
//    freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
    int n,s,a,b;
    cin >> n;
    int count = 0;
    for(int i = 1;i<=n;i++){
        for(int j = 1;j<=n;j++){
            cin >> a;
            if(a == 1){
                count++;
            }
        }
    }
    cout << count/2;
//    fclose(stdin); fclose(stdout);
    return 0;
}
