#include <bits/stdc++.h>
using namespace std;
 
int main(){
    //freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
    int n,m,a,b,x,y;
    cin >> n >> m;
    vector<vector<int>>adj(101);
    for(int i = 0;i<m;i++){
        cin >> x >> y;
        adj[x].push_back(y);
    }
    cin >> a >> b;
    vector<int>visited(101,0);
    queue<int>q;
    q.push(a);
    visited[a] = 1;
    int f = 0;
    while(!q.empty()){
        int v = q.front();
        q.pop();
        if(v == b){
            cout << "YES" << "\n";
            f = 1;
            break;
        }
        for(int i = 0;i<adj[v].size();i++){
            if(!visited[adj[v][i]]){
                q.push(adj[v][i]);
                visited[adj[v][i]] = 1;
            }
        }
    }
    if(f == 0){
        cout << "NO" << "\n";
    }
    //fclose(stdin); fclose(stdout);
    return 0;
}
